{
  "name": "TabZip",
  "version": "0.1.0",
  "manifest_version": 3,
  "description": "Save all tabs in a window to a single string, and load them back later.",
  "author": "Retosila",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_title": "TabZip",
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "clipboardWrite"]
}
